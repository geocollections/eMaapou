{
  "compilerOptions": {
    "target": "ES2018",
    "module": "ESNext",
    "moduleResolution": "Node",
    "lib": [
      "ESNext",
      "ESNext.AsyncIterable",
      "DOM"
    ],
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
    },
    "types": [
      "@nuxt/types",
      "@nuxtjs/i18n",
      "@nuxtjs/axios",
      "@nuxtjs/vuetify",
    ],
  },
  // "vueCompilerOptions": {
    // "target": 2,
    // "target": 2, // For Vue version <= 2.6.14
  // },
  // "vueCompilerOptions": {
  //   "experimentalDisableTemplateSupport": true
  // },
  "exclude": ["node_modules", ".nuxt", "dist"]
}
